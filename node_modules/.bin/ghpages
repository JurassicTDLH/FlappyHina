#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/ghpages@0.0.10/node_modules/ghpages/node_modules:/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/ghpages@0.0.10/node_modules:/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/ghpages@0.0.10/node_modules/ghpages/node_modules:/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/ghpages@0.0.10/node_modules:/mnt/c/Users/fqc20/Documents/VueTrain/Flappy/FlappyHina/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ghpages@0.0.10/node_modules/ghpages/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ghpages@0.0.10/node_modules/ghpages/cli.js" "$@"
fi
